{
  "name": "ipod-music-backend",
  "version": "1.0.0",
  "description": "Optional backend server for iPod Music App - manages OAuth and aggregates music services",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t ipod-music-backend .",
    "docker:run": "docker run -p 3001:3001 --env-file .env ipod-music-backend"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "spotify-web-api-node": "^5.0.2",
    "subsonic": "^0.0.3",
    "youtubei.js": "^15.1.1",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
